<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>ReactivePrism</id>
    <version>0.4</version>
    <title>ReactivePrism</title>
    <authors>okazuki</authors>
    <owners>okazuki</owners>
    <licenseUrl>https://github.com/runceel/ReactivePrism/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/runceel/ReactivePrism</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ReactivePrism is ReactiveProperty + Prism + InteractionRequestExtensions class. InteractionRequestExtensions convert InteractionRequest into IObservable.</description>
    <summary />
    <language>en-US</language>
    <tags>rx mvvm async rx-main prism</tags>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="ReactiveProperty" version="1.0" />
        <dependency id="Prism" />
      </group>
      <group targetFramework="wpa81">
        <dependency id="ReactiveProperty" version="1.0" />
        <dependency id="Prism.StoreApps" version="2.0" />
      </group>
      <group targetFramework="win81">
        <dependency id="ReactiveProperty" version="1.0" />
        <dependency id="Prism.StoreApps" version="2.0"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="../LICENSE.txt" target="." />
    <file src="../README.md" target="." />
    <!-- platform -->
    <!-- WPF NET45 -->
    <file src="..\bin\WPF\ReactivePrism.WPF.dll" target="lib\net45" />
    <file src="..\bin\WPF\ReactivePrism.WPF.XML" target="lib\net45" />

    <!-- WinRT -->
    <file src="..\bin\Win\ReactivePrism.Win.dll" target="lib\win81" />
    <file src="..\bin\Win\ReactivePrism.Win.XML" target="lib\win81" />
    <file src="..\bin\Win\Microsoft.Practices.Prism.Interactivity.dll" target="lib\win81" />
    <file src="..\bin\Win\Microsoft.Practices.Prism.Interactivity.xml" target="lib\win81" />

    <!-- WPA81 -->
    <file src="..\bin\WPA\ReactivePrism.WPA.dll" target="lib\wpa81" />
    <file src="..\bin\WPA\ReactivePrism.WPA.XML" target="lib\wpa81" />
    <file src="..\bin\WPA\Microsoft.Practices.Prism.Interactivity.dll" target="lib\wpa81" />
    <file src="..\bin\WPA\Microsoft.Practices.Prism.Interactivity.xml" target="lib\wpa81" />
  </files>
</package>